@model MySql.Data.MySqlClient.MySqlDataReader
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Hello World</h1>
    <h2>@ViewBag.a</h2>
    <div> 
        <table>
            <tr>
                <td>id</td>
                <td>name</td>
            </tr>
           @{ while (Model.Read())
               {
            <tr>
                <td>@Model["id"]</td>
                <td>@Model["name"]</td>
            </tr>
               }
        }
        </table>
    </div>
</body>
</html>
